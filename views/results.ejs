<% include partials/header.ejs %>
</head>
<body>
 <main>

<form action="/search">
    <input size="10" id="keyword" type="text" value="<%=keyword%>" placeholder="Type keyword" name="keyword"/> 
    <br>
    <button>Search</button>
    <a id="favLink" href="/displayKeywords">See Favorites</a>
</form>

<% imageURLs.forEach(function(imageURL, i){ %>
    <%- (i%3==0)?'<br>':'' %>
    <div  class="imageContainer">
        <img class="image" src='<%=imageURL %>' width="200" height="200">
        <img class="favoriteIcon" src="img/fav_off.png" width="20">
    </div>
 <% }) %>


</main>
<% include partials/footer.ejs %>